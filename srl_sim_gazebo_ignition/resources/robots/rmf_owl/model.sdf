<?xml version="1.0"?>
<sdf version="1.6">
  <model name="rmf_owl">
    <pose>0 0 0.053302 0 0 0</pose>
    <link name="rmf_owl/base_link">
      <inertial>
        <mass>1.46</mass>
        <inertia>
          <ixx>0.017</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.018</iyy>
          <iyz>0</iyz>
          <izz>0.028</izz>
        </inertia>
      </inertial>
      <collision name="rmf_owl/base_link_inertia_collision">
        <geometry>
          <box>
            <size>0.47 0.47 0.11</size>
          </box>
        </geometry>
      </collision>
      <visual name="rmf_owl/base_link_inertia_visual">
        <geometry>
          <mesh>
            <uri>meshes/rmf_owl.dae</uri>
          </mesh>
        </geometry>
      </visual>

      <!--Outlook of realsense_d435i/d455-->
      <collision name="realsense_visual">
        <pose frame="">0.0285 0 0.0328 0 0 1.57</pose>
        <geometry>
            <mesh>
                <uri>meshes/realsense.dae</uri>
            </mesh>
        </geometry>
      </collision>
      <visual name="realsense_visual">
        <pose frame="">0.04 0 0.036 0 0 1.57</pose>
        <geometry>
            <mesh>
                <uri>meshes/realsense.dae</uri>
            </mesh>
        </geometry>
      </visual>

      <sensor name="imu_sensor" type="imu">
          <pose>0.0 0.0 0.0 0 0 0</pose>
          <always_on>true</always_on>
          <topic>imu</topic>
          <update_rate>200</update_rate>
          <imu>
              <!--
                  <mean> and <std> are parameters of Gaussian noise
                  <bias_mean> and <bias_stddev> are parameters of Gaussian bias
                  <dynamic_bias_stddev> and <dynamic_bias_correlation_time> are parameters of noise, which drives slow derivation of sensor bias
                  <precision> is the precision of Gaussian quantized output signal
              -->
              <angular_velocity>
                  <x>
                      <noise type="gaussian">
                          <mean>0</mean>
                          <stddev>0.00</stddev>
                          <bias_mean>0.00</bias_mean>
                          <bias_stddev>0.00</bias_stddev>
                          <dynamic_bias_stddev>0.00</dynamic_bias_stddev>
                          <dynamic_bias_correlation_time>400.0</dynamic_bias_correlation_time>
                          <precision>0.00</precision>
                      </noise>
                  </x>
                  <y>
                      <noise type="gaussian">
                          <mean>0</mean>
                          <stddev>0.00</stddev>
                          <bias_mean>0.00</bias_mean>
                          <bias_stddev>0.00</bias_stddev>
                          <dynamic_bias_stddev>0.00</dynamic_bias_stddev>
                          <dynamic_bias_correlation_time>400.0</dynamic_bias_correlation_time>
                          <precision>0.00</precision>
                      </noise>
                  </y>
                  <z>
                      <noise type="gaussian">
                          <mean>0</mean>
                          <stddev>0.00</stddev>
                          <bias_mean>0.00</bias_mean>
                          <bias_stddev>0.00</bias_stddev>
                          <dynamic_bias_stddev>0.00</dynamic_bias_stddev>
                          <dynamic_bias_correlation_time>400.0</dynamic_bias_correlation_time>
                          <precision>0.00</precision>
                      </noise>
                  </z>
              </angular_velocity>
              <linear_acceleration>
                  <x>
                      <noise type="gaussian">
                          <mean>0.0</mean>
                          <stddev>0.00</stddev>
                          <bias_mean>0.0</bias_mean>
                          <bias_stddev>0.00</bias_stddev>
                          <dynamic_bias_stddev>0.00</dynamic_bias_stddev>
                          <dynamic_bias_correlation_time>175.0</dynamic_bias_correlation_time>
                          <precision>0.00</precision>
                      </noise>
                  </x>
                  <y>
                      <noise type="gaussian">
                          <mean>0.0</mean>
                          <stddev>0.00</stddev>
                          <bias_mean>0.0</bias_mean>
                          <bias_stddev>0.00</bias_stddev>
                          <dynamic_bias_stddev>0.00</dynamic_bias_stddev>
                          <dynamic_bias_correlation_time>175.0</dynamic_bias_correlation_time>
                          <precision>0.00</precision>
                      </noise>
                  </y>
                  <z>
                      <noise type="gaussian">
                          <mean>0.0</mean>
                          <stddev>0.00</stddev>
                          <bias_mean>0.0</bias_mean>
                          <bias_stddev>0.00</bias_stddev>
                          <dynamic_bias_stddev>0.00</dynamic_bias_stddev>
                          <dynamic_bias_correlation_time>175.0</dynamic_bias_correlation_time>
                          <precision>0.00</precision>
                      </noise>
                  </z>
              </linear_acceleration>
          </imu>
      </sensor>

      <!--Depth of realsense-->
      <sensor name="camera_depth" type="depth_camera">
          <pose>0.180 0 -0.025 0 0 0</pose>
          <update_rate>20</update_rate>
          <camera name="camera_depth">
              <horizontal_fov>2.09</horizontal_fov>
              <image>
                  <width>640</width>
                  <height>240</height>
                  <format>R_FLOAT32</format>
              </image>
              <clip>
                  <near>0.2</near>
                  <far>20.0</far>
              </clip>
          </camera>
      </sensor>

       <!--Left camera of realsense-->
            <sensor name="camera_left" type="camera">
                <pose>0.2 0.0475 0.2 0 0 0</pose>
                <always_on>true</always_on>
                <update_rate>10</update_rate>
                <visualize>true</visualize>
                <camera name="camera_left">
                    <!--
                        calculation of horizontal_fov:
                        h_fov = 2*tan-1(width/(2*fx))
                    -->
                    <horizontal_fov>1.37274</horizontal_fov>
                    <lens>
                        <intrinsics>
                            <fx>390.598938</fx>
                            <fy>390.598938</fy>
                            <cx>320.581665</cx>
                            <cy>237.712845</cy>
                            <s>0</s>
                        </intrinsics>
                    </lens>
                    <image>
                        <width>640</width>
                        <height>480</height>
                        <format>L8</format>
                    </image>
                </camera>
            </sensor>

            <!--Right camera of realsense-->
            <sensor name="camera_right" type="camera">
                <pose>0.2 -0.0475 0.2 0 0 0</pose>
                <always_on>true</always_on>
                <update_rate>10</update_rate>
                <visualize>true</visualize>
                <camera name="camera_right">
                    <!--
                        calculation of horizontal_fov:
                        h_fov = 2*tan-1(width/(2*fx))
                    -->
                    <horizontal_fov>1.37274</horizontal_fov>
                    <lens>
                        <intrinsics>
                            <fx>390.598938</fx>
                            <fy>390.598938</fy>
                            <cx>320.581665</cx>
                            <cy>237.712845</cy>
                            <s>0</s>
                        </intrinsics>
                    </lens>
                    <image>
                        <width>640</width>
                        <height>480</height>
                        <format>L8</format>
                    </image>
                </camera>
            </sensor>

          <sensor name="front_laser" type="gpu_ray">
                <pose>0 0 0.05 0 0 0</pose>
                <update_rate>3</update_rate>
                <visualize>true</visualize>
                <always_on>true</always_on>
                <ray>
                    <scan>
                        <horizontal>
                            <samples>1469</samples>
                            <resolution>1</resolution>
                            <min_angle>0</min_angle>
                            <max_angle>3.1459</max_angle>
                        </horizontal>
                        <vertical>
                            <samples>34</samples>
                            <resolution>1</resolution>
                            <min_angle>0</min_angle>
                            <max_angle>3.1459</max_angle>
                        </vertical>
                    </scan>
                    <range>
                        <min>0.8</min>
                        <max>30</max><!--Extracted from Simon dataset-->
                        <resolution>0.01</resolution>
                    </range>
                    <noise>
                        <type>gaussian</type>
                        <mean>0</mean>
                        <stddev>0.03</stddev>
                    </noise>
                </ray>
          </sensor>

    </link>

    <link name="rmf_owl/rotor_0">
